{
  "name": "szemul/bootstrap",
  "description": "Bootstrap interface for szemul/framework",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "phpstan/phpstan": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Szemul\\Bootstrap\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": false
  }
}
